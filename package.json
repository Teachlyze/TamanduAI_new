{
  "name": "tamanduai-platform",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "description": "Plataforma educacional enterprise com IA avançada, segurança robusta e experiência excepcional",
  "keywords": [
    "education",
    "ai",
    "edtech",
    "react",
    "supabase",
    "plagiarism-detection",
    "learning-platform"
  ],
  "author": "TamanduAI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tamanduai/platform.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:performance": "lighthouse-ci",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "npx vite-bundle-analyzer",
    "lighthouse": "lighthouse-ci",
    "metrics": "node scripts/metrics.js",
    "db:generate-types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/database.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db push",
    "db:seed": "node scripts/seed.js",
    "db:test-connection": "node scripts/test-supabase-connection.js",
    "deploy:dev": "vercel --prod",
    "deploy:staging": "vercel --prod --scope staging",
    "deploy:prod": "node scripts/deploy.js",
    "backup:create": "node scripts/backup.js create",
    "backup:list": "node scripts/backup.js list",
    "backup:restore": "node scripts/backup.js restore",
    "backup:cleanup": "node scripts/backup.js cleanup",
    "security:audit": "npm audit",
    "security:scan": "snyk test",
    "security:fix": "snyk fix",
    "docs:generate": "node scripts/generate-docs.js",
    "docs:serve": "npx docsify-cli serve docs",
    "clean": "rm -rf dist node_modules/.vite .nyc_output coverage",
    "clean:all": "npm run clean && rm -rf node_modules",
    "prepare": "husky install",
    "prepublishOnly": "npm run test:all && npm run build"
  },
  "dependencies": {
    "@agora-js/media": "^4.24.0",
    "@agora-js/report": "^4.24.0",
    "@agora-js/shared": "^4.24.0",
